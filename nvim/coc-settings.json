{
    "languageserver": {
        "rust": {
            "command": "ra_lsp_server",
            "filetypes": ["rust"],
            "rootPatterns": ["Cargo.toml"]
        },
        "clangd": {
            "command": "clangd",
            "args": ["--background-index"],
            "rootPatterns": ["compile_flags.txt", "compile_commands.json", ".vim/", ".git/", ".hg/"],
            "filetypes": ["c", "cpp", "objc", "objcpp"]
        },
        "haskell": {
            "command": "hie-wrapper",
            "rootPatterns": [
                ".stack.yaml",
                "cabal.config",
                "package.yaml"
            ],
            "filetypes": [
                "hs",
                "lhs",
                "haskell"
            ],
            "initializationOptions": {
                "languageServerHaskell": {
                    "hlintOn": true
                }
            }
        },
        "golang": {
            "command": "gopls",
            "rootPatterns": ["go.mod"],
            "filetypes": ["go"],
            "initializationOptions": {
                "usePlaceholders": true
            }
        }
    },
    "suggest.noselect": false,
    "yank.enableCompletion" : false,
    "go.goplsPath": "/home/oatiz/go/bin/gopls",
    "java.home": "/usr/lib/jvm/java-8-jdk",
    "java.jdt.ls.vmargs": "-javaagent:/home/oatiz/.m2/repository/org/projectlombok/lombok/1.16.20/lombok-1.16.20.jar -Xbootclasspath/a:/home/oatiz/.m2/repository/org/projectlombok/lombok/1.16.20/lombok-1.16.20.jar",
    "java.errors.incompleteClasspath.severity": "error",
    "java.configuration.maven.userSettings": "/home/oatiz/.m2/settings.xml",
    "java.signatureHelp.enabled": true,
    "java.contentProvider.preferred": "fernflower",
    "java.format.settings.url": "/home/oatiz/Documents/code-style/eclipse-java-google-style.xml",
    "java.format.enabled": true,
    "java.format.onType.enabled": true,
    "java.completion.enabled": true,
    "java.completion.guessMethodArguments": true,
    "java.maven.downloadSources": true
}
